<!DOCTYPE html>
<html lang="en">
<head>
    <title>Subjects | @ViewBag.courseName</title>
    <meta http-equiv="content-type" content="text/html" charset="UTF-8" />

    <!-------------stylesheet ------------------>
    <link rel="stylesheet"
          type="text/css"
          href="~/css/subjects.css"
          media="only screen and (min-width:901px)" />

    <!-------------Fav-Icon------------------>
    <link rel="icon"
          href="~/images/favicon.ico"
          type="image/gif"
          sizes="16x16" />

    <!-------------google-fonts ------------------>
    <link href="https://fonts.googleapis.com/css2?family=Arvo&family=Lato&family=Open+Sans+Condensed:wght@300&family=Source+Code+Pro&family=Ubuntu+Condensed&family=Ubuntu:wght@300&display=swap"
          rel="stylesheet" />

    <script src="https://cdn.jsdelivr.net/npm/bootstrap@5.0.0-beta1/dist/js/bootstrap.bundle.min.js"
            integrity="sha384-ygbV9kiqUc6oa4msXn9868pTtWMgiQaeYH7/t7LECLbyPA2x65Kgf80OJFdroafW"
            crossorigin="anonymous"></script>

    <link rel="stylesheet"
          href="https://pro.fontawesome.com/releases/v5.10.0/css/all.css"
          integrity="sha384-AYmEC3Yw5cVb3ZcuHtOA93w35dYTsvhLPVnYs9eStHfGJvOvKxVfELGroGkvsg+p"
          crossorigin="anonymous" />

    <link rel="stylesheet"
          href="https://pro.fontawesome.com/releases/v5.10.0/css/all.css"
          integrity="sha384-AYmEC3Yw5cVb3ZcuHtOA93w35dYTsvhLPVnYs9eStHfGJvOvKxVfELGroGkvsg+p"
          crossorigin="anonymous" />

    <link href="https://fonts.googleapis.com/css2?family=Arvo&family=Lato&family=Open+Sans+Condensed:wght@300&family=Source+Code+Pro&family=Ubuntu+Condensed&family=Ubuntu:wght@300&display=swap"
          rel="stylesheet" />
    <script src="https://cdn.jsdelivr.net/npm/bootstrap@5.0.0-beta1/dist/js/bootstrap.bundle.min.js"
            integrity="sha384-ygbV9kiqUc6oa4msXn9868pTtWMgiQaeYH7/t7LECLbyPA2x65Kgf80OJFdroafW"
            crossorigin="anonymous"></script>
    <link href="https://cdn.jsdelivr.net/npm/bootstrap@5.0.0-beta1/dist/css/bootstrap.min.css"
          rel="stylesheet"
          integrity="sha384-giJF6kkoqNQ00vy+HMDP7azOuL0xtbfIcaT9wjKHr8RbDVddVHyTfAAsrekwKmP1"
          crossorigin="anonymous" />
    <link rel="stylesheet"
          href="https://pro.fontawesome.com/releases/v5.10.0/css/all.css"
          integrity="sha384-AYmEC3Yw5cVb3ZcuHtOA93w35dYTsvhLPVnYs9eStHfGJvOvKxVfELGroGkvsg+p"
          crossorigin="anonymous" />

    <script src="https://code.jquery.com/jquery-3.2.1.slim.min.js"
            integrity="sha384-KJ3o2DKtIkvYIK3UENzmM7KCkRr/rE9/Qpg6aAZGJwFDMVNA/GpGFF93hXpG5KkN"
            crossorigin="anonymous"></script>
    <script src="https://cdnjs.cloudflare.com/ajax/libs/popper.js/1.12.9/umd/popper.min.js"
            integrity="sha384-ApNbgh9B+Y1QKtv3Rn7W3mgPxhU9K/ScQsAP7hUibX39j7fakFPskvXusvfa0b4Q"
            crossorigin="anonymous"></script>
    <script src="https://maxcdn.bootstrapcdn.com/bootstrap/4.0.0/js/bootstrap.min.js"
            integrity="sha384-JZR6Spejh4U02d8jOt6vLEHfe/JQGiRRSQQxSfFWpi1MquVdAyjUar5+76PVCmYl"
            crossorigin="anonymous"></script>

</head>
<body>
    <div id="particles-js">
        <div class="wrapperrr">
            <div class="mainCourse">
                <div class="navbarCourses-container">
                    <div class="navbarCourses">
                        <div class="logocoursescontainer">
                            <a href="/home" class="logotext"><span class="logospan">FCIT </span> HelpDesk</a>
                        </div>

                        <div class="navMenuCoursesMain">
                            <ul class="navmenucourses">
                                <li>
                                    <a href="Home" target="_blank">home</a>
                                </li>
                                <li><a href="/about">About</a></li>
                                <li><a href="/courses" style="color: #00adb5">courses</a></li>
                                <li><a href="/team">Team</a></li>
                                <li><a href="/socialmedia">Updates</a></li>
                                <li><a href="/contact">contact</a></li>
                                <li><a href="/contribution">contribute</a></li>
                                <li>
                                    <div class="dropdownCourse">
                                        <button class="dropdownMenuButton btn dropdown-toggle"
                                                type="button"
                                                id="dropdownMenuButton"
                                                data-toggle="dropdown"
                                                aria-haspopup="true"
                                                aria-expanded="false">
                                            @ViewBag.user.Fname
                                        </button>
                                        <div class="dropdown-menu"
                                             aria-labelledby="dropdownMenuButton">
                                            <a class="dropdown-item-Courses" href="/Home/logout">Log out</a>
                                        </div>
                                    </div>
                                </li>
                            </ul>
                        </div>
                    </div>
                </div>

                <div class="coursesNote">
                    <p style="color:white;text-align:center;font-size:25px;margin-top:50px">
                        @ViewBag.courseName
                    </p>

                    <p></p>
                </div>
                <script>
                    let is_admin = "@ViewBag.user.Is_admin";
                    if (is_admin === "Y") {
                        document.getElementById("adminPanel").style.display = "block";
                    }
                </script>
                @{
                    if (ViewBag.contribs.Count == 0)
                    {
                        <h3 style="color:white;text-align:center">No Material Available Yet :/</h3>
                    }
                    else
                    {
                        <div class="courses-container">
                            <div class="courses-container-sub">
                                <ul>
                                    @for (var i = 0; i < ViewBag.contribs.Count; i++)
                                    {
                                        <li>
                                            <div class="upper">
                                                <p class="crsTitle">@ViewBag.contribs[i].title</p>
                                                <p class="crsAuthor">@ViewBag.contribs[i].type</p>
                                                <p class="ContributorNameUpper">Contributed By @ViewBag.contribs[i].fname </p>
                                                <p class="stars">⭐⭐⭐⭐</p>
                                            </div>
                                            <div class="lower text-center">
                                                <a href="/Course/ShowMedia/?path=@ViewBag.contribs[i].path" class="lowerA">Preview</a>
                                                <a href="/Course/DownloadMedia/?path=@ViewBag.contribs[i].path" class="lowerB">Download</a>
                                            </div>
                                        </li>
                                    }
                                </ul>
                            </div>
                        </div>
                    }
                }
            </div>
        </div>
</body>
<script type="text/javascript" src="~/js/main.js"></script>
<script type="text/javascript" src="~/js/particles.js"></script>
<script type="text/javascript" src="~/js/app.js"></script>
</html>
